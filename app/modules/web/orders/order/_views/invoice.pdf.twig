<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=windows-1252">

    <style>


        /*=== Resets ===*/
        *{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}
        *:before,*:after{/*-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box*/ display: none;}
        html{font-size:8px;-webkit-tap-highlight-color:rgba(0,0,0,0)}
        body{font-family:"Helvetica Neue",Helvetica,Arial,sans-serif;font-size:14px;line-height:1.42857143;color:#0b121e;background-color:#fff}
        input,button,select,textarea{font-family:inherit;font-size:inherit;line-height:inherit}
        a{color:#266cb1;text-decoration:none}
        a:hover,a:focus{color:#294f9f;text-decoration:underline}
        a:focus{outline:thin dotted;outline:5px auto -webkit-focus-ring-color;outline-offset:-2px}
        figure{margin:0}
        img{vertical-align:middle}
        .img-responsive{display:block;max-width:100%;height:auto}
        hr{ width: 100%; margin: auto; margin-top:10px;margin-bottom:10px;border:0;border-top:1px solid #eee}
        [role="button"]{cursor:pointer}
        h1,h2,h3,h4,h5,h6,
        .h1,.h2,.h3,.h4,.h5,.h6{font-family:"Helvetica Neue",Helvetica,Arial,sans-serif;line-height:1.1;}
        h1 small,h2 small,h3 small,h4 small,h5 small,h6 small,
        .h1 small,.h2 small,.h3 small,.h4 small,.h5 small,.h6 small,
        h1 .small,h2 .small,h3 .small,h4 .small,h5 .small,h6 .small,
        .h1 .small,.h2 .small,.h3 .small,.h4 .small,.h5 .small,.h6 .small{font-weight:normal;line-height:1;color:#777}
        h1,.h1,h2,.h2,h3,.h3{margin-top:20px;margin-bottom:10px}
        h1 small,.h1 small,h2 small,.h2 small,h3 small,.h3 small,h1 .small,.h1 .small,h2 .small,.h2 .small,h3 .small,.h3 .small{font-size:65%; }
        h4,.h4,h5,.h5,h6,.h6{margin-top:10px;margin-bottom:10px}
        h4 small,.h4 small,h5 small,.h5 small,h6 small,.h6 small,h4 .small,.h4 .small,h5 .small,.h5 .small,h6 .small,.h6 .small{font-size:75%; }
        h1,.h1{font-size:36px}
        h2,.h2{font-size:30px}
        h3,.h3{font-size:24px}
        h4,.h4{font-size:18px}
        h5,.h5{font-size:14px}
        h6,.h6{font-size:12px}
        p{margin:5px 0 5px}
        small,.small{font-size:65%;}
        .text-left{text-align:left}
        .text-right{text-align:right}
        .text-center{text-align:center}
        .text-justify{text-align:justify}
        .text-nowrap{white-space:nowrap}

        span.code {width: 100%; color: #666; font-family: monospace; word-wrap: break-word;}

        .list-unstyled{padding-left:0;list-style:none}
        .list-inline{padding-left:0;margin-left:-5px;list-style:none}
        .list-inline>li{display:inline-block;padding-right:5px;padding-left:5px}

        .container{width: 100%; max-width:650px; margin-right:auto;margin-left:auto;padding-left:15px;padding-right:15px; float: none;}
        .row{margin-left:-15px;margin-right:-15px; float: none;}

        table{width:100%; max-width:100%; margin-bottom: 0; background-color:transparent; border-collapse:collapse; border-spacing:0}
        table th, table td {vertical-align: top; padding: 0 !important; float: none;}

                                                                                      table.collapsed {margin-bottom: 0;}
        table.collapsed th, table.collapsed td {margin: 0; padding: 1px 4px !important;}
        table.table {margin: 15px 0}
        table.table th, .table td {border: 1px solid #FFF; padding: 4px 7px !important; vertical-align: middle;}
        table.table th {background: #3b4e87; color: #FFF;}
        table.table td {background: #FAFAFA}
        table.table th.invisible, .table td.invisible {border: none !important; background: transparent !important}

        table.table td.unstyled {background: transparent; border-color: transparent; padding: 0;}

        table.table tfoot td {background: #e4e8f3}
        table.table tfoot th {background: #edf1f9; color: #3b4e87}
        table.table tfoot td.total {background: #d3dcea; font-size: 14px; font-weight: bold;}
        table.table tfoot th.total {background: #c7d2e5; font-size: 14px; font-weight: bold;}

        .valign-top {vertical-align: top !important;}
        .valign-middle {vertical-align: middle !important;}
        .valign-bottom {vertical-align: bottom !important;}

        .container:before,.container:after,.row:before,.row:after{content:" ";display:table}
        .container:after,.row:after{ clear: both;}

        .clearfix:before,.clearfix:after {content:" ";display:table}
        .clearfix:after {clear:both}

        .col-100, .col-90, .col-80, .col-70, .col-60, .col-50, .col-40, .col-30, .col-20, .col-10, .col-third {}
        .col-100 {width: 100%;}
        .col-90 {width: 90%;}
        .col-80 {width: 80%;}
        .col-70 {width: 70%;}
        .col-60 {width: 60%;}
        .col-50 {width: 50%;}
        .col-40 {width: 40%;}
        .col-30 {width: 30%;}
        .col-20 {width: 20%;}
        .col-10 {width: 10%;}

        .col-25 {width: 25%;}
        .col-75 {width: 75%;}

        .col-third {width: 33.33%;}


        /*=== Layout ===*/

        @page {
            margin: 200px 50px 80px;
        }

        #header {
            position: fixed;
            left: -50px;
            top: -180px;
            right: -50px;
            padding: 30px 0 0;
        }
        #header h3 {margin: 0 0 10px; color: #0b121e}
        #header td {vertical-align: bottom}
        #header img {max-width: 200px;}
        #header .row {padding-bottom: 10px; border-bottom: 3px solid #ffc114;}
        #header .table {margin: 0;}
        #header .header-table th {background: transparent; color: #294f9f;}
        #header .header-table td {background: transparent;}

        .order-top [class^="status-"] {
            font-size: 18px;
            font-weight: bold;
        }
        .status-pending   {color: #FF931E}
        .status-cancelled {color: #CC0514}
        .status-finished  {color: #1AB516}
        .status-closed    {color: #004AB5}


        #body {
            margin: 40px 0 0;
        }
        #body .table thead th,
        #body .table tbody td {
            padding: 15px 12px !important;
        }
        #body .table tbody td {
            background: transparent;
        }
        #body .table tfoot {
            border-top: 2px solid #294f9f;
        }
        #body .table tfoot td {
            border: none;
        }

        #body .divider {
            border: 2px solid #F5F5F5;
            margin: 40px 0;
        }
        #body .order-details {
            padding: 40px 0 0;
        }
        #body .details-block h5 {
            color: #294f9f;
        }
        #body .details-block p {
            display: block;
            padding: 0 10px;
            color: #6d7178;
        }


        #footer {
            position: fixed;
            left: -50px;
            bottom: 0;
            right: -50px;
            padding: 10px;
            color: #161E28;
        }
        #footer .container {
            position: relative;
        }
        #footer .divider {
            position:  absolute;
            top: -15px ;left: 125px; right: 105px;
            border: 2px solid #ffc114;
        }
        #footer p {
            display: block;
            margin: 0 0 5px;
            font-size: 12px;
        }
        #footer .footer-info-text {
            color: rgba(11,18,30,0.5);
            font-size: 14px;
        }
    </style>

    <title>Purchase Order #{{ WOrderID }}</title>

</head>

<body class="text-center">

<!--HEADER-->
<div id="header">
    <div class="container">
        <div class="row">

            <table>
                <tr>
                    <td class="col-100 text-center" colspan="2">
                        <h3>PURCHASE ORDER</h3>
                    </td>
                </tr>
                <tr>
                    <td class="col-50 valign-middle">
                        <img src="data:image/png;base64," title="GlobalBag" alt="GlobalBag" >
                    </td>
                    <td class="col-50 valign-middle">
                        <table class="table header-table">
                            <tr>
                                <th class="text-right">Date</th>
                                <td>{{ WOrderInfo.WOrderDate|date("F jS Y")  }}</td>
                            </tr>
                            <tr>
                                <th class="text-right">Order No.</th>
                                <td>{{ WOrderID }}</td>
                            </tr>
                            <tr>
                                <th class="text-right">Status</th>
                                <td><strong class="{{ WOrderInfo.WOrderStatusInfo.class }}">{{ WOrderInfo.WOrderStatusInfo.name }}</strong></td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>

        </div>
    </div>
</div>

<!--FOOTER-->
<div id="footer" class="text-center">
    <div class="container">
        <div class="divider"></div>
        <div class="row">
            <p>For any question or issues regarding this order, please get in touch with us:</p>
            <p class="footer-info-text">GlobalBag Sales Team, (555) 555 5555 - 555 5555 5555, sales@globalbagusa.com</p>
        </div>
    </div>
</div>


<!--BODY-->
<div id="body">
    <div class="container">

        <div class="row">

            <table class="table user-area-table">
                <thead>
                <tr>
                    <th>Qty</th>
                    <th>SKU</th>
                    <th class="col-100 text-left">Product name</th>
                    <th>Price</th>
                    <th>Subtotal</th>
                </tr>
                </thead>
                <tbody>
                {% for i in 0..2 %}
                {% for ItemInfo in WOrderInfo.WOrderItems %}
                    <tr>
                        <td class="text-nowrap text-center">{{ ItemInfo.WOItemQty }}</td>
                        <td class="text-nowrap text-center">{{ ItemInfo.ProductCode }}</td>
                        <td class="text-nowrap fill">{{ ItemInfo.WOItemName }}</td>
                        <td class="text-nowrap text-right">${{ ItemInfo.WOItemPriceUSD|number_format(2, '.', ',') }}</td>
                        <td class="text-nowrap text-right">$ {{ ItemInfo.WOItemTotal|number_format(2, '.', ',') }}</td>
                    </tr>
                {% endfor %}
                {% endfor %}
                </tbody>
                <tfoot>
                <tr>
                    <td class="invisible">&nbsp;</td>
                    <th class="invisible text-nowrap text-right" colspan="3">Subtotal</th>
                    <td class="text-nowrap text-right">${{ WOrderInfo.WOrderTotalAmount|number_format(2, '.', ',') }}</td>
                </tr>
                <tr>
                    <td class="invisible">&nbsp;</td>
                    <th class="invisible text-nowrap text-right" colspan="3">VAT</th>
                    <td class="text-nowrap text-right">$ {{ WOrderInfo.WOrderTotalTax }}</td>
                </tr>
                <tr>
                    <td class="invisible">&nbsp;</td>
                    <th class="invisible text-nowrap text-right" colspan="3">Shipping and handling</th>
                    <td class="text-nowrap text-right">$ {{ WOrderInfo.WOrderTotalShipping }}</td>
                </tr>
                <tr>
                    <td class="invisible">&nbsp;</td>
                    <th class="invisible text-nowrap text-right" colspan="3">Total</th>
                    <td class="text-nowrap text-right">$ {{ WOrderInfo.WOrderTotalTotal|number_format(2, '.', ',') }}</td>
                </tr>
                </tfoot>
            </table>

            <div class="divider"></div>

            <table class="text-left order-details">
                <tbody>
                <tr>
                    <td class="col-third">
                        <div class="details-block">
                            <h5>Shipping Address</h5>
                            <p>{{ WOrderInfo.WOrderShippingContactName }}<br>
                                {{ WOrderInfo.WOrderShippingAddress }}<br>
                                {{ WOrderInfo.WOrderShippingAddress2 }}<br>
                                {{ WOrderInfo.WOrderShippingAddressCity }}<br>
                                {{ WOrderInfo.WOrderShippingAddressZipCode }}<br>
                                {{ WOrderInfo.WOrderShippingAddressCountryName }}</p>
                            <p>{{ WOrderInfo.WOrderShippingContactPhone }} </p>
                        </div>
                    </td>
                    <td class="col-third">
                        <div class="details-block">
                            <h5>Billing Address</h5>
                            <p>{{ WOrderInfo.WOrderContactFullName }}<br>
                                {{ WOrderInfo.WOrderAddress1 }}<br>

                                {% if WOrderInfo.WOrderAddressAddress2 %}
                                    {{ WOrderInfo.WOrderAddressAddress2 }}<br>
                                {% endif  %}
                                {{ WOrderInfo.WOrderAddressCity }}<br>
                                {{ WOrderInfo.WOrderAddressZipCode }}<br>
                                {{ WOrderInfo.WOrderAddressCountryName }}</p>
                            <p>{{ WOrderInfo.WOrderContactPhone }}</p>
                        </div>
                    </td>
                    <td class="col-third">
                        <div class="details-block">
                            <h5>Payment method</h5>
                            <p>{{ WOrderInfo.WOrderOptionPayment }}</p>
                        </div>

                        <div class="details-block">
                            <h5>Shipping Method</h5>
                            <p>Flat rate - Fixed</p>
                            <p>Tracking ID: <a href="/"><i class="fas fa-fw fa-external-link-alt"></i> 951753852654</a></p>
                        </div>
                    </td>
                </tr>
                </tbody>
            </table>

        </div>
    </div>
</div>

</body>
</html>
